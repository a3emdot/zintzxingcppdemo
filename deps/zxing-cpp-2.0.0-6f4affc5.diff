+		{39, 16, 64, 14, 14,   {36, {{1, 62 }, {0, 0}}}},
+		{40, 20, 36, 18, 16,   {28, {{1, 44 }, {0, 0}}}},
+		{41, 20, 44, 18, 20,   {34, {{1, 56 }, {0, 0}}}},
+		{42, 20, 64, 18, 14,   {42, {{1, 84 }, {0, 0}}}},
