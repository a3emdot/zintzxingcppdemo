+enum class ErrorCorrectionLevel
+{
+	Low,     // L = ~7 % correction
+	Medium,  // M = ~15% correction
+	Quality, // Q = ~25% correction
+	High,    // H = ~30% correction
+	Invalid, // denotes in invalid/unknown value
+};
