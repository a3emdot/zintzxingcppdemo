+		{5, 18, 18, 16, 16,    {14, {{1, 18 }, {0, 0}}}},
+		{6, 20, 20, 18, 18,    {18, {{1, 22 }, {0, 0}}}},
+		{7, 22, 22, 20, 20,    {20, {{1, 30 }, {0, 0}}}},
